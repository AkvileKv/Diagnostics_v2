<%- include('partials/header') %>

<!-- Title -->
<section class="blue-section" id="title">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="blue-space-search">
          <h1>LEAF-MINING NEPTICULIDAE OF CENTRAL ASIA</h1>
        </div>
      </div>
    </div>
  </div>
</section>

<!-- empty space-->
<section class="clear-section">
  <div class="container-fluid">
    <div class="empty-space3">
      <p class="intro"> </p>
    </div>
  </div>
</section>

<div class="container">
  <div class="jumbotron text-center">

    <form action="/s-central-asia" method="get">

      <p class="search-text-div">FIND A KNOWN SPECIES</p>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="">SPECIES NAME</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <input type="text" name="dspecies" class="form-control form-control-lg text-center" placeholder="type without a genus name" autocomplete="off">
        </div>
      </div>

      <div class="search-text-div">
        <p>OR SELECT THE CHARACTERS AND GET MATCHING SPECIES</p>
        <p class="search-below">You can miss some characters but it makes the diagnostics less precise</p>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">HOST-PLANT FAMILY</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12 character-select">
          <select name="dhostplantfamily" class="select-gray form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Not selected or unknown">Not selected or unknown</option>
            <option class="others" value="Anacardiaceae">Anacardiaceae</option>
            <option class="others" value="Asteraceae">Asteraceae</option>
            <option class="others" value="Betulaceae">Betulaceae</option>
            <option class="others" value="Ericaceae">Ericaceae</option>
            <option class="others" value="Fabaceae">Fabaceae</option>
            <option class="others" value="Fagaceae">Fagaceae</option>
            <option class="others" value="Lamiaceae">Lamiaceae</option>
            <option class="others" value="Malvaceae">Malvaceae</option>
            <option class="others" value="Myrtaceae">Myrtaceae</option>
            <option class="others" value="Rhamnaceae">Rhamnaceae</option>
            <option class="others" value="Rosaceae">Rosaceae</option>
            <option class="others" value="Salicaceae">Salicaceae</option>
            <option class="others" value="Sapindaceae (=Aceraceae)">Sapindaceae (=Aceraceae)</option>
            <option class="others" value="Ulmaceae (=Cannabaceae)">Ulmaceae (=Cannabaceae)</option>
            <option class="others" value="Urticaceae">Urticaceae</option>
            <option class="others" value="Another family, not included in this list">Another family, not included in this list</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">FOREWING</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dforewing" class="select-gray2 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Without a pattern, almost uniform">Without a pattern, almost uniform</option>
            <option class="others" value="With a conspicuous fascia(e) but no spots">With a conspicuous fascia(e) but no spots</option>
            <option class="others" value="With a dark, false fascia(e)">With a dark, false fascia(e)</option>
            <option class="others" value="With a spot(s) but no fascia">With a spot(s) but no fascia</option>
            <option class="others" value="With a fascia and spot(s)">With a fascia and spot(s)</option>
            <option class="others" value="Irrorated or sparsely speckled">Irrorated or sparsely speckled</option>
            <option class="others" value="Other">Other</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">TEGUMEN</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dtegumen" class="select-gray3 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Band-shaped, pseuduncus absent">Band-shaped, pseuduncus absent</option>
            <option class="others" value="Extended into a one-lobed pseuduncus">Extended into a one-lobed pseuduncus</option>
            <option class="others" value="Extended into a bifid or trifid pseuduncus">Extended into a bifid or trifid pseuduncus</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">UNCUS</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="duncus" class="select-gray4 form-select form-control-lg">
            <option style="color:gray;" value="null">select a character</option>
            <option class="others" value="One-lobed">One-lobed</option>
            <option class="others" value="Bilobed or with 2 conspicuous papillae">Bilobed or with 2 conspicuous papillae</option>
            <option class="others" value="With 3–4 conspicuous papillae">With 3–4 conspicuous papillae</option>
            <option class="others" value="Inverted Y- or V-shaped<">Inverted Y- or V-shaped</option>
            <option class="others" value="Other">Other</option>
            <option class="others" value="Uncus absent">Uncus absent</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">GNATHOS</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dgnathos" class="select-gray5 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Widely U-shaped">Widely U-shaped</option>
            <option class="others" value="Inverted V- or Y-shaped">Inverted V- or Y-shaped</option>
            <option class="others" value="With one short caudal process or lobe">With one short caudal process or lobe</option>
            <option class="others" value="With one long caudal process">With one long caudal process</option>
            <option class="others" value="With two long caudal processes">With two long caudal processes</option>
            <option class="others" value="H-shaped, with conspicuous anterior processes">H-shaped, with conspicuous anterior processes</option>
            <option class="others" value="With a large caudal lobe">With a large caudal lobe</option>
            <option class="others" value="Other">Other</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">VALVA</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dvalva" class="select-gray6 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Without an apical process">Without an apical process</option>
            <option class="others" value="With one short or medium short apical process">With one short or medium short apical process</option>
            <option class="others" value="With one long or very long apical process">With one long or very long apical process</option>
            <option class="others" value="With two apical processes">With two apical processes</option>
            <option class="others" value="With a median or basal, slender or lobe-like process(es)">With a median or basal, slender or lobe-like process(es)</option>
            <option class="others" value="Valva deeply divided, two-folded">Valva deeply divided, two-folded</option>
            <option class="others" value="Other">Other</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">JUXTA</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="djuxta" class="select-gray7 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Absent or mostly membranous, inconspicuous">Absent or mostly membranous, inconspicuous</option>
            <option class="others" value="Distinctly thickened, at least partially">Distinctly thickened, at least partially</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">TRANSTILLA</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dtranstilla" class="select-gray8 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Without sublateral processes">Without sublateral processes</option>
            <option class="others" value="With short or weakly differentiated processes">With short or weakly differentiated processes</option>
            <option class="others" value="With long but slender sublateral processes">With long but slender sublateral processes</option>
            <option class="others" value="With long but wide sublateral processes">With long but wide sublateral processes</option>
            <option class="others" value="Absent">Absent</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-lg-4 col-md-6 col-sm-12 character-name">
          <label for="inputState">VINCULUM</label>
        </div>
        <div class="col-lg-8 col-md-6 col-sm-12">
          <select name="dvinculum" class="select-gray9 form-select form-control-lg">
            <option style="color:gray" value="null">select a character</option>
            <option class="others" value="Short or medium short; lateral lobes short or absent">Short or medium short; lateral lobes short or absent</option>
            <option class="others" value="Short or medium short; lateral lobes long">Short or medium short; lateral lobes long</option>
            <option class="others" value="Long or very long, with lateral lobes">Long or very long, with lateral lobes</option>
            <option class="others" value="Long or very long, without lateral lobes">Long or very long, without lateral lobes</option>
          </select>
        </div>
      </div>

      <div class="form-row">
        <div class="col-md-4 col-sm-12 character-name">
          <label for="inputState">PHALLUS</label>
        </div>
        <div class="col-md-4 col-sm-12">
          <select name="dphalluswithoutcarinae" class="select-gray10 form-select form-control-lg">
            <option style="color:gray" value="null">without distinctive carinae</option>
            <option class="others" value="Without cornuti or only a few small cornuti">Without cornuti or only a few small cornuti</option>
            <option class="others" value="With numerous small but loose cornuti">With numerous small but loose cornuti</option>
            <option class="others" value="With numerous small cornuti in a compact band(s)">With numerous small cornuti in a compact band(s)</option>
            <option class="others" value="With several spine-like but loose cornuti">With several spine-like but loose cornuti</option>
            <option class="others" value="With many large but loose cornuti">With many large but loose cornuti</option>
            <option class="others" value="With many large + small but loose cornuti">With many large + small but loose cornuti</option>
            <option class="others" value="With large or large + small cornuti in a compact band">With large or large + small cornuti in a compact band</option>
            <option class="others" value="With differentiated cornuti in separate clusters">With differentiated cornuti in separate clusters</option>
            <option class="others" value="With 1–3 loose horn-like cornuti">With 1–3 loose horn-like cornuti</option>
            <option class="others" value="Other">Other</option>
          </select>
        </div>
        <div class="col-md-4 col-sm-12">
          <select name="dphalluswithcarinae" class="select-gray11 form-select form-control-lg">
            <option style="color:gray" value="null">with distinctive carinae</option>
            <option class="others" value="With only ventral carinae and without or a very few cornuti">With only ventral carinae and without or a very few cornuti</option>
            <option class="others" value="With only two dentate plates and without or very few cornuti">With only two dentate plates and without or very few cornuti</option>
            <option class="others" value="With ventral + lateral carinae and without or some very small cornuti">With ventral + lateral carinae and without or some very small cornuti</option>
            <option class="others" value="With ventral and/or lateral carinae and horn-like + small cornuti">With ventral and/or lateral carinae and horn-like + small cornuti</option>
            <option class="others" value="Other">Other</option>
          </select>
        </div>
      </div>
      <div class="form-row">
        <input type="text" name="dregion" value="Central Asia" hidden>
      </div>

      <button type="submit" class="btn yellow-button w-100">SEARCH</button>

    </form>

  </div>
</div>



<%- include('partials/footer') %>
