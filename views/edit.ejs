<%- include("partials/header2"); -%>
<%- include("partials/admin-header"); -%>

<div class="container">
  <div class="jumbotron text-center">

    <i class="fas fa-edit fa-6x"></i>
    <h1 class="display-3">Edit</h1>
    <p class="records-text">NeptiDB</p>

    <%  neptis.forEach(function(nepti){ %>

      <div class="database-result">
        <%=nepti.species%>
        <a href="/<%= nepti._id %>">Edit</a>

        <form action="/delete" method="post">
            <button class="btn-dark btn-sm" type="submit" name="deleteById" onclick="return confirm('Are you sure you want to delete this record?');" value="<%=nepti._id%>">Delete</button>
        </form>

      </div>





      <% }) %>
  </div>
</div>

<%- include("partials/footer"); -%>
